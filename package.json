{
  "name": "hanshot",
  "version": "0.0.0",
  "private": true,
  "main": "src/main.dist.js",
  "dependencies": {
    "cropperjs": "^0.7.0",
    "jimp": "^0.2.24",
    "lodash": "^4.6.1",
    "mkdirp": "^0.5.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "redux": "^3.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "electron-packager": "^7.1.0",
    "electron-prebuilt": "^1.0.2",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-config-airbnb-base": "^2.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "json-loader": "^0.5.4",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "tape": "^4.5.1",
    "webpack": "^1.13.1"
  },
  "scripts": {
    "build": "webpack",
    "start": "npm run build && electron .",
    "package": "npn run build && node package.js",
    "lint": "eslint src/cache.js test",
    "test": "npm run test:unit && npm run test:int && npm run test:int:node",
    "test:unit": "HANSHOT_TEST=true tape -r babel-register -r ./test/unit/helpers/fake-electron.js './test/unit/**/*.test.js'",
    "test:int": "HANSHOT_TEST=true node ./test/integration/setup/run.js",
    "test:int:node": "HANSHOT_TEST=true tape -r babel-register './test/int-node/**/*.test.js'"
  }
}
